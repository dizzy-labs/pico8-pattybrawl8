pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
--patty brawl 8
--save the day and fight the bad guys
function _init()
 is_flip=false--is the sprite flipped
 max_jump=24--max jump height in pixels
 frame_num=0
 gravity=0.9
 
 bullets={}--bullets table
 fire_held=false
 
 player={}--player table
 player.x=8
 player.y=32
 player.sprite=0
 player.h_spd=0.65
 player.v_spd=0.80
 player.j_height=0
 player.standing=true
 player.jumping=false
end

function collision(direction)
 --up
 if direction == 2 then
  return fget(mget(player.x/8,(player.y-1)/8),0) or fget(mget(player.x/8+1,(player.y-1)/8),0)
 --down
 elseif direction == 3 then
  return fget(mget(player.x/8,player.y/8+2),0) or fget(mget(player.x/8+1,player.y/8+2),0)
 --left
 elseif direction == 0 then
  return fget(mget((player.x-1)/8,player.y/8),0) or fget(mget((player.x-1)/8,(player.y+7)/8+1),0)
 --right
 elseif direction == 1 then
  return fget(mget((player.x+7)/8+1,player.y/8),0) or fget(mget((player.x+7)/8+1,(player.y+7)/8+1),0)
 end
end

function fire()
 local bul = {
  x = player.x+8,
  y = player.y+8,
  sprite = 34,
  is_flip= is_flip,
  dx = 1
 }
 if is_flip then
  bul.dx*=-1
  bul.x-=8
 end
 add(bullets,bul)
 player.sprite=2
end

function move()
 --move left
 if btn(0) then
  is_flip=true
  if not collision(0) then
   player.x -= player.h_spd
   player.walking=true
  end
 end
 --move right
 if btn(1) then
  is_flip=false
  if not collision(1) then
   player.x+=player.h_spd
   player.walking=true
  end
 end
 --jump
 if btn(2) and (player.standing or player.jumping) then
  if player.j_height<max_jump then
   if not collision(2) then
    player.jumping=true
    player.y-=player.v_spd
   end
   player.j_height+=player.v_spd
  else
   player.jumping=false
  end
 else
  player.jumping=false
  player.j_height=0
 end
 --fire
 if btn(5) and #bullets<5 then
  if not fire_held then
   fire_held=true
   fire()
  end
 else
  fire_held=false
 end
 --gravity
 if not (player.jumping or player.standing) then
 player.y+=gravity
 end
end

function anim()
 --walk anim
 if player.walking and player.standing then
  player.sprite=player.sprite%4+2
 --jump anim
 elseif player.jumping then
  player.sprite+=2
  if player.sprite<6 or player.sprite>8 then
   player.sprite=6 
  end
 else
  player.sprite=0
 end
end

function _update60()
 player.walking=false
 player.standing=collision(3)
 move()
 for bul in all(bullets) do
  bul.x += bul.dx
  if bul.x > player.x+64
  or bul.x < player.x-64 then
   del(bullets,bul)
  end
 end
 if frame_num%12==0 then
  anim()
 end
 frame_num+=1
end

function _draw()
 cls()
 camera(player.x-64,player.y-64)
 spr(player.sprite,player.x,player.y,2,2,is_flip)
 map(0,0,0,0,128,32)
 for bul in all(bullets) do
  spr(bul.sprite,bul.x,bul.y,1,1,bul.is_flip
  )
 end
-- print("||",player.x,player.y)
-- print(player.standing, player.x-48,player.y)
-- print(player.jumping, player.x-48,player.y+16)
-- print(collision(2),player.x,player.y-24)
-- print(collision(3),player.x,player.y+24)
-- print(collision(0),player.x-24,player.y)
-- print(collision(1),player.x+24,player.y)
end

